# Environment Configuration
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# ===================================
# AUTHENTICATION
# ===================================
REACT_APP_LOGIN_EMAIL=your-admin@email.com
REACT_APP_LOGIN_PASSWORD=your-secure-password

# ===================================
# API ENDPOINTS
# ===================================

# PDF Service
REACT_APP_PDF_SERVICE_URL=https://your-pdf-service.onrender.com/api/generate-pdf

# Main Data Webhooks
REACT_APP_AIRTABLE_WEBHOOK=https://your-n8n-instance.com/webhook/airtable
REACT_APP_MONTHLY_WEBHOOK=https://your-n8n-instance.com/webhook/monthly-performance
REACT_APP_SOURCES_WEBHOOK=https://your-n8n-instance.com/webhook/sources
REACT_APP_CONSULTANT_META_ADS_WEBHOOK=https://your-n8n-instance.com/webhook/consultant-meta-ads

# Meta Ads Webhooks (by account)
REACT_APP_META_ADS_BEAUTY=https://your-n8n-instance.com/webhook/meta-ads-beauty
REACT_APP_META_ADS_FOOD=https://your-n8n-instance.com/webhook/meta-ads-food
REACT_APP_META_ADS_RECREATION=https://your-n8n-instance.com/webhook/meta-ads-recreation
REACT_APP_META_ADS_HOME=https://your-n8n-instance.com/webhook/meta-ads-home
REACT_APP_META_ADS_PET=https://your-n8n-instance.com/webhook/meta-ads-pet
REACT_APP_META_ADS_FINANCIAL=https://your-n8n-instance.com/webhook/meta-ads-financial

# Refresh Hook
REACT_APP_REFRESH_HOOK=https://your-n8n-instance.com/webhook/refresh

# ===================================
# BACKEND CONFIGURATION
# ===================================
PORT=5000
NODE_ENV=production
